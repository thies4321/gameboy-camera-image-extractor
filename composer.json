{
    "name": "thies/gameboy-camera-image-extractor",
    "description": "This is a PHP library for extracting Game Boy Camera photos from a save ram (.sav) file and writing them in various image formats.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thies Verhave"
        }
    ],
    "require": {
        "php": "^8.4",
        "ext-gd": "*"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.45",
        "phpunit/phpunit": "^11.5",
        "psy/psysh": "^0.12.7",
        "phpstan/phpstan": "^2.1"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "GameboyCameraImageExtractor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GameboyCameraImageExtractor\\Tests\\": "tests/Unit"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }
}
